@model ABB.Catalogo.Entidades.Core.Usuario

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Usuario</h2>

<h3>¿Está seguro que desea eliminar este usuario?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.CodUsuario)</dt>
        <dd>@Html.DisplayFor(model => model.CodUsuario)</dd>

        <dt>@Html.DisplayNameFor(model => model.Nombres)</dt>
        <dd>@Html.DisplayFor(model => model.Nombres)</dd>

        <dt>@Html.DisplayNameFor(model => model.DesRol)</dt>
        <dd>@Html.DisplayFor(model => model.DesRol)</dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" />
            @Html.ActionLink("Volver a la lista", "Index", null, new { @class = "btn btn-default" })
        </div>
    }
</div>
